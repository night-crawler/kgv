{{#* inline "header" }}
    {{# with this.status }}
        <ul>
            <li><em>Observed Chart Name</em>: {{ observedChartName }}</li>
            <li><em>Observed Generation</em>: {{ observedGeneration }}</li>
            <li><em>Observed Source Artifact Revision</em>: {{ observedSourceArtifactRevision }}</li>
            <li><em>URL</em>: {{ url }}</li>
        </ul>
    {{/ with }}
{{/ inline }}

{{#* inline "content" }}
    <h2>Spec</h2>
    <pre>{{ to_yaml this.spec }}</pre>
{{/ inline }}

{{> base }}
