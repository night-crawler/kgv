{{#* inline "header" }}
    {{# with this.status }}
        <ul>
            <li><em>Fully Labeled Replicas</em>: {{ fullyLabeledReplicas }}</li>
            <li><em>Observed Generation</em>: {{ observedGeneration }}</li>
            <li><em>Replicas</em>: {{ replicas }}</li>
        </ul>
    {{/ with }}
{{/ inline }}

{{#* inline "content" }}
    <h2>Spec</h2>
    <pre>{{ to_yaml this.spec }}</pre>
{{/ inline }}

{{> base }}
